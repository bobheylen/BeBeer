<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.PickUpFragment">

    <ImageButton
        android:id="@+id/btnGetPikup"
        android:layout_width="328dp"
        android:layout_height="268dp"
        android:src="@drawable/emptiesgoodsize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btnMakePikup"
        android:layout_width="328dp"
        android:layout_height="268dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/knopgetajobgoodsize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnGetPikup" />

</androidx.constraintlayout.widget.ConstraintLayout>